{
    "name": "igorwanbarros/project-laravel-base",
    "description": "Project Laravel/Lumen Base.",
    "keywords": ["project", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
	    "igorwanbarros/base-laravel": "*",
	    "igorwanbarros/autenticacao-laravel": "*",
	    "igorwanbarros/php2html-laravel": "*",
	    "igorwanbarros/templates-laravel": "*",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
